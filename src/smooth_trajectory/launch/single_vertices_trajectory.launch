<?xml version="1.0"?>
<launch>

  <node name="Trajectory" pkg="smooth_trajectory" type="single_vertices_trajectory.py" respawn="false" output="screen">
    <param name="object_type" type="string" value="linear"/>
    <param name="folder_name" value="vertex_rec"/>
    <param name="show_single_vertex_plot" type="bool" value="true"/>
    <param name="show_all_vertices_plot" type="bool" value="true"/>
    <param name="csv_ref_name" value="ref_data_rec.csv"/>
    <param name="circ_radius" type="double" value="0.025"/>  <!-- 0.065 0.025-->
    <param name="vertices_num" type="int" value="4"/>  

  </node>


</launch>
